  command = /snap/clion/415/bin/cmake/linux/x64/bin/cmake --regenerate-during-build -S/home/activeintriosea/CLionProjects/SDLTesting -B/home/activeintriosea/CLionProjects/SDLTesting/cmake-build-debug
  description = Re-running CMake...
  generator = 1


#############################################
# Rule for re-checking globbed directories.

rule VERIFY_GLOBS
  command = /snap/clion/415/bin/cmake/linux/x64/bin/cmake -P /home/activeintriosea/CLionProjects/SDLTesting/cmake-build-debug/CMakeFiles/VerifyGlobs.cmake
  description = Re-checking globbed directories...
  generator = 1


